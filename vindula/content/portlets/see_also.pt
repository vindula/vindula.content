<dl class="portlet" tal:define="view view/data">

    <dt class="portletHeader">
        <span class="portletTopLeft"></span>
        <span class="tile" tal:content="view/get_title">Portlet Veja Tambem</span>
        <span class="portletTopRight"></span>
    </dt>
    <dd class="portletItem" >
        <div id="_portlet_seealso">
            <ul>
                <tal:rep repeat="item view/getItens">
                    <li><a  tal:define="ctx item/content/getObject"
                        tal:attributes="href ctx/absolute_url"
                        tal:content= "ctx/Title">
                    </a></li>
                </tal:rep>
            </ul>

        </div>
    </dd>
    <dd class="portletFooter"></dd>
    <div class="visualClear"></div>
</dl>

